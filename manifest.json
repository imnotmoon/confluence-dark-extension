{
  "name": "Confluence Tint",
  "version": "0.1",
  "description": "눈 아픈 라이너를 위한 Confluence 다크모드 익스텐션",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  }
}